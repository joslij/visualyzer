{
  "name": "visualyzer-web",
  "version": "1.0.0",
  "description": "The Visualyzer web application",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf ../../wwwroot/web && cd ../../wwwroot && mkdir web",
    "build": "webpack --config webpack.prod.js",
    "postbuild": "cp -r dist/* ../../wwwroot/web",
    "build:dev": "webpack --config webpack.dev.js",
    "start": "webpack serve --config webpack.prod.js",
    "start:dev": "webpack serve --config webpack.dev.js --open chrome",
    "test": "prettier --check . && eslint . && jest --no-cache --passWithNoTests",
    "test:dev": "prettier --write . && eslint . && jest --watchAll --no-cache --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joslij/visualyzer.git"
  },
  "keywords": [
    "react",
    "javascript"
  ],
  "author": "LJ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joslij/visualyzer/issues"
  },
  "homepage": "https://github.com/joslij/visualyzer#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.4.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^11.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3",
    "style-loader": "^2.0.0",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "core-js": "^3.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
