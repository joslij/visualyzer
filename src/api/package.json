{
  "name": "visualyzer-api",
  "version": "1.0.0",
  "description": "The API of Visualyzer",
  "main": "index.js",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf node_modules && rm -rf ../../wwwroot/*",
    "build": "cd ../../wwwroot && npm ci",
    "test": "cd ../../wwwroot && jest --no-cache --passWithNoTests",
    "start": "set NODE_ENV=production&& node index.js",
    "build:ci": "npm i",
    "test:ci": "jest --no-cache --passWithNoTests",
    "test:dev": "jest --watchAll --no-cache --passWithNoTests",
    "start:dev": "set NODE_ENV=development&& nodemon index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joslij/visualyzer.git"
  },
  "keywords": [
    "visual",
    "cognitive",
    "azure",
    "nodejs"
  ],
  "author": "LJ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joslij/visualyzer/issues"
  },
  "homepage": "https://github.com/joslij/visualyzer#readme",
  "dependencies": {
    "@azure/cognitiveservices-computervision": "^8.0.0",
    "@azure/cosmos": "^3.11.0",
    "@azure/identity": "^1.3.0",
    "@azure/keyvault-secrets": "^4.1.0",
    "@azure/storage-blob": "^12.5.0",
    "@types/dotenv": "^8.2.0",
    "async": "^3.2.0",
    "bcryptjs": "^2.4.3",
    "bent": "^7.3.12",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.1",
    "helmet": "^4.6.0",
    "html-escaper": "^3.0.3",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0"
  }
}
